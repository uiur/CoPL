      }
    }
  };
  Z evalto Z by E-Const {};
  S(S(S(S(Z)))) times Z is Z by T-Succ {
    S(S(S(Z))) times Z is Z by T-Succ {
      S(S(Z)) times Z is Z by T-Succ {
        S(Z) times Z is Z by T-Succ {
          Z times Z is Z by T-Zero {};
          Z plus Z is Z by P-Zero {}
        };
        Z plus Z is Z by P-Zero {}
      };
      Z plus Z is Z by P-Zero {}
    };
      Z plus Z is Z by P-Zero {}
  }
}

// 20
